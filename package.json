    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.14.0",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "^1.8.6",
    "screenfull": "^5.1.0",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.2.1",
    "twilio-video": "^2.15.3",
    "typescript": "^4.1.2",
    "ua-parser-js": "^0.7.28",
    "use-sound": "^2.0.2",
    "yup": "^0.29.3"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "prebuild": "yarn run generate-build-version",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node ./src/helpers/copyPDFTronAssets.js && craco start",
    "build": "node ./src/helpers/copyPDFTronAssets.js && craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint ./",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "prettier --tab-width 4 --trailing-comma es5 --print-width 120 --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5",
    "babel-loader": "8.1.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/stories/**",
      "!**/assets/**",
      "!**/_tests_/**",
      "!**/constants/styles/**",
      "!src/*.{ts,tsx}"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/node-logger": "^6.0.26",
    "@storybook/preset-ant-design": "^0.0.1",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.0.28",
    "@types/react-router-dom": "^5.1.6",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.4.0",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.3.0",
    "react-is": "^16.13.1"
  }
}
